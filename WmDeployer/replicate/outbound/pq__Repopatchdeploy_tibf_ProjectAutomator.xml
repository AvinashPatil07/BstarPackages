<?xml version="1.0" encoding="UTF-8"?><DeployerSpec exitOnError="true" sourceType="Repository">
<DeployerServer>
<host>${machinename}:${isport}</host>
<user>${Username}</user>
<pwd>${Password}</pwd>
<!--<pwdHandle>$(PasswordHandle)</pwdHandle>-->
</DeployerServer>
<Environment>
<Repository>
<repalias name="Git_Repo">
<type>FlatFile</type>
<urlOrDirectory>D:\demo</urlOrDirectory>
<Test>false</Test>
</repalias>
</Repository>
<IS>
<isalias name="Local">
<host>LP-R901F8E2</host>
<port>5555</port>
<user>Administrator</user>
<pwd>${Password}</pwd>
<!--<pwdHandle>$(PasswordHandle)</pwdHandle>-->
<useSSL>false</useSSL>
<version>9.12</version>
<installDeployerResource>false</installDeployerResource>
<Test>false</Test>
<executeACL>Internal</executeACL>
</isalias>
</IS>
</Environment>
<Projects projectPrefix="">
<Project description="" name="Repo_patch_deploy" overwrite="false" type="Repository">
<ProjectProperties>
<Property name="projectLocking">false</Property>
<Property name="concurrentDeployment">true</Property>
<Property name="ignoreMissingDependencies">false</Property>
<Property name="isTransactionalDeployment">true</Property>
</ProjectProperties>
<DeploymentSet autoResolve="full" description="" name="myDeploymentSet" srcAlias="Git_Repo">
<Component componentType="isflowservice" compositeName="BstarBPM" name="BstarBPM.Services:Servicepatch" srcAlias="Git_Repo" type="IS"/>
</DeploymentSet>
<DeploymentMap description="" name="myDeploymentMap"/>
<MapSetMapping mapName="myDeploymentMap" setName="myDeploymentSet">
<alias type="IS">Local</alias>
</MapSetMapping>
<DeploymentCandidate description="" mapName="myDeploymentMap" name="myDeployment"/>
</Project>
</Projects>
</DeployerSpec>
